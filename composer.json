{
    "name": "koutamercado/laravel-custom-helpers",
    "type": "library",
    "authors": [
        {
            "name": "Carlos Mercado",
            "email": "koutamercado@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "koutamercado\\laravelCustomHelpers\\": ""
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "koutamercado\\laravelCustomHelpers\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "symfony/var-dumper": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6|^7"
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the ddd function (~4.0)."
    }
}
